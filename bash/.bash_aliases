# General Utilities

alias 'sauce'='source $HOME/.bashrc && echo "Reloaded profile"'

alias 'eza'='eza --color=auto --icons=auto --time-style=long-iso --header --group-directories-first --sort=name --git --git-repos-no-status --show-symlinks'

alias 'ls'='eza'
alias 'l'='eza'
alias 'll'='eza --long'
alias 'la'='eza --all'
alias 'lla'='eza --long --all'

alias 'tree'='eza --tree --ignore-glob=.git'
alias 't'='tree'
alias 'lt'='tree --long'
alias 'tl'='tree --long'
alias 'ta'='tree --all'
alias 'tla'='tree --long --all'
alias 'tal'='tree --long --all'

alias 'fm'='dolphin'

alias 'grep'='grep --color=auto'

# Interactive Commands

alias 'cp'='cp --interactive'
alias 'mv'='mv --interactive'
alias 'ln'='ln --interactive'
alias 'rm'='rm --interactive'

# Navigation

alias '..'='z ../'
alias '...'='z ../../'
alias '....'='z ../../../'
alias '.....'='z ../../../../'
alias '......'='z ../../../../../'

